<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ArcLoader">
        <attr name="size" format="dimension" />
        <attr name="cap" format="enum" >
            <enum name="BUTT" value="0"/>
            <enum name="ROUND" value="1"/>
            <enum name="SQUARE" value="2"/>
        </attr>
        <attr name="color" format="color" />
        <attr name="total" format="float" />
        <attr name="current" format="float" />
    </declare-styleable>

    <declare-styleable name="AnimatedArcLoader">
        <attr name="progressPerSecond" format="float" />
    </declare-styleable>
</resources>